    "shellHost": "https://localhost",
    "consumer": "ACME_CORP",

    "clientAppId": 2,
    "SOCKET_IO_PORT": 3002,
    "defaultauth": "cd-auth",
    "initialPage": "dashboard",
    "mfManifestPath": "/assets/mf.manifest.json",

    "apiOptions": {
